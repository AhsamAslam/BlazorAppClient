        }
        else
        {
            TxtType = InputType.Password;
            type = "password";
        }
    }

    public void onCreateTextBox()
    {
        this.txtbox2.AddIcon("append", "e-input-eye");
        if (TxtType == InputType.Password)
        {
            TxtType = InputType.Text;
            type = "text";
        }
        else
        {
            TxtType = InputType.Password;
